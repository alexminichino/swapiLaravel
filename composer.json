{
    "name": "alexminichino/swapi",
    "description": "The swapi package",
    "authors": [
        {
            "name": "Alex",
            "email": "alexminichino@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "AlexMinichino\\Swapi\\Tests\\": "tests",
            "AlexMinichino\\Swapi\\": "src",
            "Database\\Seeders\\": "database/seeds/"
        }
    },
    "autoload-dev": {
       "psr-4": {
         "AlexMinichino\\Swapi\\Tests\\": "tests",
         "AlexMinichino\\Swapi\\": "src"
       }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexMinichino\\Swapi\\Config\\SwapiServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "orchestra/testbench": "^4.0"
    }
}
